<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asus1.menuList">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="arefin.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="arefin.CreateActivity" />
        <activity
            android:name="arefin.CollectorActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="arefin.ItemListActivity"
            android:label="@string/title_activity_item_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="arefin.MenuCreatorActivity"></activity>
    </application>

</manifest>